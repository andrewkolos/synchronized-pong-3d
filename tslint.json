{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-clean-code"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "object-literal-sort-keys": false,
        "variable-name": false,
        "ordered-imports": false,
        "max-line-length": [
            true,
            {
                "limit": 120,
                "ignore-pattern": "^import [^,]+ from |^export | implements "
            }
        ]
    },
    "rulesDirectory": []
}